angeTitle\" v-bind:value=\"titleItem\">\n        <button class=\"btn btn-primary ml-3\" v-on:click=\"addTodo\"> {{this.$store.state.btnEdit}} </button>\n    </div>     \n</template>\n<script>\n    export default {\n        name: 'Add',\n        computed:{\n            titleItem(){\n                return this.$store.state.titleTemp\n            }\n        },\n        props: {\n            editItem:Function,\n            addItem: Function\n        },\n        methods: {\n            changeTitle(event){\n                var e = event.target.value\n                this.$store.commit('',e)\n            },\n            addTodo : function(){\n                if(!(this.titleItem.length<=0)){\n                    if(this.$store.state.btnEdit=='add'){\n                        this.addItem({id: this.$store.state.todos.length +1,title: this.titleTemp ,done:false,prop:false})\n                    }\n                    else{\n                        this.editItem(this.titleTemp)\n                    }\n                }\n                this.$store.state.titleTemp =''\n            }\n        },\n    }\n</script>"],"sourceRoot":"src/components"}]}